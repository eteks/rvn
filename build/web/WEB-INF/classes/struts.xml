<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="example.xml"/>
    <!-- Configuration for the default package. -->
    <constant name="struts.devMode" value="true" />
    <package name="json" extends="json-default" namespace="/">
        <action name="createvehicleversion" class="com.controller.Vehicle_and_Model" method="CreateVehicleVersion">
            <result type="json" />
        </action> 
        <action name="loadpreviousvehicleversion_data" class="com.controller.Vehicle_and_Model" method="LoadPreviousVehicleversionData">
            <!--<result>vehicle_add.jsp</result>-->
            <result type="json" />
        </action>  
        <action name="UserLoginProcess" class="com.controller.login.LoginController" method="login">
            <result type="json" />
        </action> 
        
        <!-- admin module action   r s -->
        <action name="groupCreation" class="com.controller.admin.CreateUserGroupCtrl" method="createUserGroup">
            <result type="json" />
        </action> 
         <action name="getAllUserGroup" class="com.controller.admin.CreateUserGroupCtrl" method="getAllUserGroup">
            <result type="json" />
        </action>
        
        <action name="moduleCreation" class="com.controller.admin.UserModuleController" method="createModule">
            <result type="json" />
        </action> 
         <action name="getAllUserModule" class="com.controller.admin.UserModuleController" method="getAllUserModule">
            <result type="json" />
        </action>
        <action name="getModuleName" class="com.controller.admin.ModulePermisionController" method="getAllModuleName">
            <result type="json" />
        </action>
        <action name="CreateModulePermision" class="com.controller.admin.ModulePermisionController" method="CreateModulePermision">
            <result type="json" />
        </action>
        <action name="getModule_Group" class="com.controller.admin.GroupPermisionController" method="getAllGroup_Model">
            <result type="json" />
        </action>
       <action name="getSelectedModulePermission" class="com.controller.admin.GroupPermisionController" method="getPermisionByModuleId">
            <result type="json" />
       </action>
       <action name="createGroupPermission" class="com.controller.admin.GroupPermisionController" method="addGroupPermission">
            <result type="json" />
       </action>
       
        </package>

    <package name="default" extends="struts-default" namespace="/">
       <action name="logout" class="com.controller.login.LoginController"
                 method="logout">
            <result name="success">index.jsp</result>
           
        </action> 
        <action name="vehicleversion_listing" 
                class="com.controller.Vehicle_and_Model" 
                method="GetVehicleVersion_Listing">
            <result name="success">vehicle_version.jsp</result>
        </action>
         
        <action name="dashboard" class="com.controller.GlobalAction" method="GetDashboardData">
            <result>dashboard.jsp</result>
        </action>
        
        <action name="ivn_supervisor" class="com.controller.GlobalAction" method="GetDashboardData">
            <result>ivn_supervisor.jsp</result>
        </action>
        <action name="vehicle_listing" class="com.controller.Vehicle_and_Model" method="GetVehicle_Listing">
            <result>vehicle_listing.jsp</result>
        </action>
        <action name="vehiclemodel_listing" class="com.controller.Vehicle_and_Model" method="GetVehicleModel_Listing">
            <result>vehicle_model.jsp</result>
        </action>
        <action name="vehicle_add" class="com.controller.Vehicle_and_Model" method="DisplayCreateVehicleversion">
            <result>vehicle_add.jsp</result>
        </action>
        
        <action name="pdb_owner" class="com.controller.GlobalAction" method="GetDashboardData">
            <result>pdb_owner.jsp</result>
        </action>
        <action name="pdb_listing" class="com.controller.GlobalAction" method="GetDashboardData">
            <result>pdb_listing.jsp</result>
        </action>
        <action name="features_listing" class="com.controller.GlobalAction" method="GetDashboardData">
            <result>features_listing.jsp</result>
        </action>
        <action name="pdb_assign" class="com.controller.GlobalAction" method="GetDashboardData">
            <result>pdb_assign.jsp</result>
        </action>
         
        
    </package>
</struts>
