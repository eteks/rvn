                <interceptor-ref name="defaultStack"/>
                <interceptor-ref name="session"/>
            </interceptor-stack>
        </interceptors>
        <default-interceptor-ref name="sessionExpirayStack"></default-interceptor-ref>
        <action name="logout" class="com.controller.login.LoginController"
                method="logout">
            <interceptor-ref name="sessionExpirayStack" />
            <result name="session">index.jsp</result>     
            <result name="success">index.jsp</result>           
        </action>
         
        <action name="vehicleversion_listing" 
                class="com.controller.ivn_supervisor.Vehicle_and_Model" 
